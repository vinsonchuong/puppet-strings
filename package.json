{
  "name": "puppet-strings",
  "version": "0.0.0",
  "description": "A better interface for common browser automation workflows",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/puppet-strings",
  "bugs": "https://github.com/vinsonchuong/puppet-strings/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "main": "./index.js",
  "type": "module",
  "repository": "vinsonchuong/puppet-strings",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "dependencies": {
    "puppeteer-core": ">=14.1.1"
  },
  "devDependencies": {
    "ava": "^4.2.0",
    "dedent": "^0.7.0",
    "fs-extra": "^10.1.0",
    "get-stream": "^6.0.1",
    "ps-list": "^8.1.0",
    "puppet-strings-chrome": "^2.0.3",
    "semantic-release": "^19.0.2",
    "sidelifter": "^2.0.1",
    "tempy": "^3.0.0",
    "xo": "^0.49.0"
  },
  "ava": {
    "verbose": true,
    "timeout": "10m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "ava/no-import-test-files": "off",
      "import/extensions": "off",
      "import/no-anonymous-default-export": "off",
      "import/no-useless-path-segments": "off",
      "unicorn/import-index": "off",
      "unicorn/no-await-expression-member": "off",
      "unicorn/prefer-text-content": "off"
    }
  }
}
